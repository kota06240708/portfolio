{   
    
    "globals": {
        "$": true
    },
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "rules": {
        "prefer-const": 2,
        "arrow-body-style": 2,
        "arrow-spacing": 2,
        "no-empty": 2,
        "brace-style": 2,
        "comma-spacing": [2, {"before": false, "after": true}],
        "space-infix-ops": 1,
        "eqeqeq": 2,
        "no-dupe-args": 2,
        "no-cond-assign": 2,
        "no-eval": 2,
        "new-parens": 2,
        "quotes": [2,"single"],
        "semi": 2,
        "constructor-super": 2,
        "no-this-before-super": 2,
        "no-var": 2
    },
    "parserOptions": {
        "sourceType": "module"
    }
}
